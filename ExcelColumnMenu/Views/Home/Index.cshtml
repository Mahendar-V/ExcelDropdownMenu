
@model ExcelColumnMenu.Controllers.ExcelColumnDropdownOptions

<form method="post" action="/excel/exportexcel" style="margin-top:30px">
  <div class="form-group row">
    <label for="textarea" class="col-4 col-form-label">Courses Column Options</label> 
    <div class="col-8">
      <textarea  asp-for="CourseOptions"  cols="40" rows="3" class="form-control"></textarea>
    </div>
  </div>
  <div class="form-group row">
    <label for="textarea1" class="col-4 col-form-label">Institutes Column Options</label> 
    <div class="col-8">
      <textarea  asp-for="CourseInstitutionOptions"  cols="40" rows="3" class="form-control"></textarea>
    </div>
  </div>
  <div class="form-group row">
    <label for="textarea2" class="col-4 col-form-label">Course Periods Column Options</label> 
    <div class="col-8">
      <textarea  asp-for="CoursePeriodOptions"  cols="40" rows="3" class="form-control"></textarea>
    </div>
  </div> 
   <div class="form-group row">
    <label for="textarea2" class="col-4 col-form-label">Course Periods Column Options</label> 
    <div class="col-8">
      <input  asp-for="SheetName" class="form-control" type="text"/>
    </div>
  </div> 
  <div class="form-group row">
    <div class="offset-4 col-8">
      <button name="submit" type="submit" class="btn btn-primary">Export Excel Column Options</button>
    </div>
  </div>
</form>



